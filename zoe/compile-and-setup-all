cd proofs/commitment
python commitment.py > commitment.code
../../../target/release/zokrates compile --gadgets -i 'commitment.code'
../../../target/release/zokrates setup
../../../target/release/zokrates export-verifier
cd ../invalidator
python invalidator.py > invalidator.code
../../../target/release/zokrates compile --gadgets -i 'invalidator.code'
cd ../public_key
python public_key.py > public_key.code
../../../target/release/zokrates compile --gadgets -i 'public_key.code'
cd ../sha256
python sha256.py > sha256.code
../../../target/release/zokrates compile --gadgets -i 'sha256.code'
cd ../root
echo 6 | python root.py > root.code
../../../target/release/zokrates compile --gadgets -i 'root.code'
cd ../transaction
echo 6 | python transaction.py > transaction.code
../../../target/release/zokrates compile --gadgets -i 'transaction.code'
../../../target/release/zokrates setup
../../../target/release/zokrates export-verifier
cd ../withdraw
echo 6 | python withdraw.py > withdraw.code
../../../target/release/zokrates compile --gadgets -i 'withdraw.code'
../../../target/release/zokrates setup
../../../target/release/zokrates export-verifier
cd ../../
